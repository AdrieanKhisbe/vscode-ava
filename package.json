{
  "name": "ava",
  "displayName": "AVA",
  "icon": "media/icon.png",
  "version": "0.4.0",
  "description": "Snippets for AVA",
  "license": "MIT",
  "repository": "SamVerschueren/vscode-ava",
  "publisher": "samverschueren",
  "author": {
    "name": "Sam Verschueren",
    "email": "sam.verschueren@gmail.com",
    "url": "github.com/SamVerschueren"
  },
  "engines": {
    "vscode": "^0.10.6"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/ava.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/assertions.json"
      }
    ]
  },
  "scripts": {
      "vscode:prepublish": "npm run compile",
      "compile": "tsc -p ./",
      "watch": "tsc -watch -p ./",
      "postinstall": "node ./node_modules/vscode/bin/install",
      "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/node": "^10.3.3",
    "typescript": "^2.9.2",
    "vscode": "^1.1.18"
  }
}
