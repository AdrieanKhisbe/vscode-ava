{
	"scopeName": "code.ava-log",
	"fileTypes": [],
	"repository": {
		"test-status-line": {
			"name": "ava-log.test",
			"begin": "\\s\\s(?=[✔✖])",
			"end": "$",
			"patterns": [
				{
					"name": "markup.deleted.ava-log",
					"match": "✖"
				},
				{
					"name": "markup.inserted.ava-log",
					"match": "✔"
				},
				{
					"name": "variable.other.ava-log",
					"match": "(?<=›)[^›]+$"
				},
				{
					"name": "constant.language.ava-log",
					"match": "›"
				}
			]
		}, 
		"test-failure-line": {
			"name": "ava-log.failure",
			"begin": "\\s\\s(?=[⚠])",
			"end": "$",
			"patterns": [
				{
					"name": "invalid.illegal.ava-log",
					"match": "⚠"
				},
				{
					"name": "variable.other.ava-log",
					"match": "(?<=›)[^›]+$"
				},
				{
					"name": "constant.language.ava-log",
					"match": "›"
				}
			]
		},
		"test-failure-details": {
			"name": "ava-log.failure-details",
			"begin": "^\\s\\s\\s\\s",
			"end": "$",
			"patterns": [
				{
					"name": "comment.line.ava-log",
					"match": ".*"
				}
			]
		},
		"test-status": {
			"name": "ava-log.failure-details",
			"begin": "^\\s\\s(?=[◯◎◌◉])",
			"end": "$",
			"patterns": [
				{
					"name": "keyword.other.ava-log",
					"match": "◯ \\d+"
				},
				{
					"name": "support.type.ava-log",
					"match": "◎ \\d+"
				},
				{
					"name": "support.function.ava-log",
					"match": "◌ \\d+"
				},
				{
					"name": "invalid.deprecated.ava-log",
					"match": "◉ \\d+"
				}
			]
		}
	},
	"patterns": [
		{
			"name": "section-test",
			"begin": "(❯) (.*)",
			"beginCaptures": {
				"1": {
					"name": "keyword.ava-log"
				},
				"2": {
					"name": "constant.language.ava-log"
				}
			},
			"end": "^$",
			"patterns": [
				{
					"include": "#test-status-line"
				},
				{
					"include": "#test-failure-line"
				},
				{
					"include": "#test-failure-details"
				},
				{
					"include": "#test-status"
				}
			]
		}
	]
}